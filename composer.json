{
    "name": "katie-bray/wordpress-theme",
    "description": "Katie Bray WordPress Theme with Stripe Integration",
    "type": "wordpress-theme",
    "require": {
        "php": ">=7.4",
        "stripe/stripe-php": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "KatieBray\\": "includes/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
